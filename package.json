{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "lint": "DEBUG=eslint:cli-engine eslint .",
    "lint:fix": "DEBUG=eslint:cli-engine eslint . --fix",
    "server": "cd server && npm run dev",
    "app:dev": "DEV_ENV=development npx expo start -c",
    "app:prod": "DEV_ENV=production npx expo start -c",
    "test": "DEV_ENV=test jest --watchAll --silent",
