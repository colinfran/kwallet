    "core-js": "^3.8.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-rate-limit": "^6.7.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "keyv": "^4.5.2",
